call plug#begin(stdpath('config') . '/plugged')

Plug 'vim-airline/vim-airline'

Plug 'vim-airline/vim-airline-themes'

Plug 'gruvbox-community/gruvbox'

Plug 'preservim/nerdtree'

call plug#end()


color gruvbox

set nocompatible
set guifont=Consolas:h14
set number
set relativenumber
set tabstop=4
set softtabstop=4
set shiftwidth=4
set noswapfile
set nobackup
set mouse=a
set cursorline

